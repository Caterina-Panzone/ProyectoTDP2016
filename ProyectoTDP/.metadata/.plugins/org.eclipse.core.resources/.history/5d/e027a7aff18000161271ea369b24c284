package Tanques;

import Juego.*;

public class Enemigo extends Tanque{

	//Atributos
	
	protected int puntos;
	
	//Constructor
	
	public Enemigo(int vm, int vd, int resis, char direc, Celda celda, int puntos){
		super(vm, vd, resis, direc, celda);
		puntos= this.puntos;
	}
	
	//Consultas
	
	public int getPuntos(){
		return puntos;
	}
	
}
